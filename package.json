{
  "name": "astronaut-ui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./tokens": {
      "types": "./dist/tokens/index.d.ts",
      "import": "./dist/tokens/index.js"
    },
    "./button": {
      "types": "./dist/components/button/index.d.ts",
      "import": "./dist/components/button/index.js"
    },
    "./input": {
      "types": "./dist/components/input/index.d.ts",
      "import": "./dist/components/input/index.js"
    },
    "./switch": {
      "types": "./dist/components/switch/index.d.ts",
      "import": "./dist/components/switch/index.js"
    },
    "./modal": {
      "types": "./dist/components/modal/index.d.ts",
      "import": "./dist/components/modal/index.js"
    },
    "./tooltip": {
      "types": "./dist/components/tooltip/index.d.ts",
      "import": "./dist/components/tooltip/index.js"
    }
  },
  "sideEffects": [
    "./src/tokens/tokens.css",
    "./src/styles/reset.css",
    "**/*.css"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.build.json && vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "echo 'add eslint later'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test-runner": "^0.23.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.7.0",
    "axe-core": "^4.10.3",
    "jsdom": "^25.0.0",
    "playwright": "^1.55.0",
    "storybook": "^8.6.14",
    "typescript": "^5.6.2",
    "vite": "^5.4.2",
    "vitest": "^2.1.1"
  }
}
